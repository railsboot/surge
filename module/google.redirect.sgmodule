#!name=Google 302
#!desc=Google 搜索重定向

[General]
force-http-engine-hosts = %APPEND% *google.cn

[MITM]
hostname = %INSERT% google.cn, google.com.hk

[URL Rewrite]
# Google 搜索
^https?:\/\/(www\.)?google\.cn(\/)?$ https://www.google.com 302
^https?:\/\/(www\.)?google\.com\.hk(\/)?$ https://www.google.com 302

# Google 地图
^https?:\/\/maps\.google\.cn(\/)?$ https://maps.google.com 302
