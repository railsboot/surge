#!name=Google 302
#!desc=Google 搜索重定向HTTPS

[General]
force-http-engine-hosts = %APPEND% google.cn, google.com.hk, g.cn

[MITM]
hostname = %INSERT% google.cn, google.com.hk, g.cn

[URL Rewrite]
^https?:\/\/(www\.)?g\.cn(\/.*)?$ https://www.google.com$2 302
^https?:\/\/(www\.)?google\.cn(\/.*)?$ https://www.google.com$2 302
^https?:\/\/(www\.)?google\.com\.hk(\/.*)?$ https://www.google.com$2 302
